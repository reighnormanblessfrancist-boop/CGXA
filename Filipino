local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "CGXA Hub",
    Icon = 0,
    LoadingTitle = "Proyekto CGXA Hub",
    LoadingSubtitle = "ni superbadodoy",
    ShowText = "CGXA Hub",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ConfigFolder",
        FileName = "CGXA Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
        Title = "Walang Pamagat",
        Subtitle = "Sistema ng Key",
        Note = "Walang paraan para makuha ang key ang ibinigay",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"Hello"}
    }
})

-- Tab na Bahay
local HomeTab = Window:CreateTab("Bahay", "house")
local HomeLabel = HomeTab:CreateLabel("Maligayang pagdating sa bahay!", "house")
local HomeParagraph = HomeTab:CreateParagraph({Title = "Paalala", Content = "Maaaring hindi gumana ang mga script depende sa executor."})

local TimeDateSection = HomeTab:CreateSection("Oras at Petsa")
local TimeLabel = TimeDateSection:CreateLabel(os.date("%H:%M:%S"))
local DateLabel = TimeDateSection:CreateLabel(os.date("%m-%d-%y"))

local HomeSection = HomeTab:CreateSection("Bahay")
local DexButton = HomeSection:CreateButton({
    Name = "Dex",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/memeenjoyer43/darkdex/refs/heads/main/script"))()
    end
})
local debUNCButton = HomeSection:CreateButton({
    Name = "debUNC",
    Callback = function()
        loadstring(game:HttpGet("https://github.com/alyssagithub/debUNC/raw/main/debUNC.luau"))()
    end
})

